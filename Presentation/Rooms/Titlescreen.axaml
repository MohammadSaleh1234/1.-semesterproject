<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Avalonia.Rooms.Titlescreen">
    <Grid RowDefinitions="*">
	 <Image Source="avares://Presentation/Assets/Rooms/Tittlescreen.png"
	 Stretch="UniformToFill"
	 />

		<TextBlock x:Name="HighscoreText"
           HorizontalAlignment="Center"
           VerticalAlignment="Center"
           FontSize="20"
           FontWeight="ExtraBold"
           Foreground="#002F5F"
           Margin="0,100"/>


        <Button x:Name="QuitGameButton"
                Content="Quit Game"
                HorizontalAlignment="Center"
				VerticalAlignment="Bottom"
                Margin="0,100"
				Background="#004080"
                Grid.Row="0"/>
       

    </Grid>
</UserControl>
