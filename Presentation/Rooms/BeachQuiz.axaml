                   Grid.Row="0"/>
        
        <Button x:Name="GoCoralButton"
                Content="Go to the coralreef"
                HorizontalAlignment="Center"
                Margin="0,20"
                Grid.Row="1"/>
    </Grid>
</UserControl>
