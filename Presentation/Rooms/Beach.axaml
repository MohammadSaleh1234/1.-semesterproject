<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Avalonia.Rooms.Beach">
    <Grid RowDefinitions="*,Auto">
       

        <TextBlock Text="Beach Room"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   FontSize="48"
                   Grid.Row="0"/>

        <TextBlock x:Name="OutputTextInventory"
               VerticalAlignment="Top"
               HorizontalAlignment="Center"
               FontSize="24"/>
        
       
        <Button x:Name="GoQuizButton"
                Content="Go to the Quiz"
                HorizontalAlignment="Center"
                Margin="0,20"
                Grid.Row="1"/>
        
        <Button x:Name="ToolButton"
                Content="Take Trashbag"
                HorizontalAlignment="Left"
                Margin="0,20"
                Grid.Row="0"/>
    </Grid>
</UserControl>
